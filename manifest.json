{
  "manifest_version": 2,
  "name": "Catinator",
  "version": "0.1",
  "web_accessible_resources": ["kittens/*.jpg"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["kitten.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Toggle cats"
  }
}
